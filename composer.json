{
    "name": "quantaquirk/auth",
    "description": "The QuantaQuirk Auth package.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-hash": "*",
        "quantaquirk/collections": "^1.0.0",
        "quantaquirk/contracts": "^1.0.0",
        "quantaquirk/http": "^1.0.0",
        "quantaquirk/macroable": "^1.0.0",
        "quantaquirk/queue": "^1.0.0",
        "quantaquirk/support": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "QuantaQuirk\\Auth\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "quantaquirk/console": "Required to use the auth:clear-resets command (^10.0).",
        "quantaquirk/queue": "Required to fire login / logout events (^10.0).",
        "quantaquirk/session": "Required to use the session based guard (^10.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
